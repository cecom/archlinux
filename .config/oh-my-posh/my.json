{
  "osc99":true,
  "console_title":true,
  "console_title_style":"template",
  "console_title_template":"{{.User}}@{{.Host}} > {{.Folder}}",
  "final_space":false,
  "blocks":[
    {
      "type":"prompt",
      "alignment":"left",
      "segments":[
        {
          "type":"time",
