{
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "foreground": "#E5C07B",
          "properties": {
            "time_format": "15:04:05",
            "prefix": "[",
            "postfix": "]"
          }
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "#B5B50D"
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#98be65"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#61AFEF",
          "properties": {
            "style": "short"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#F3C267",
          "properties": {
            "prefix": "(",
            "postfix": ")",
            "display_status": false,
            "branch_identical_icon": "\uF14A",
            "branch_gone_icon": "\u274E"
